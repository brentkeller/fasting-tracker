{
  "expo": {
    "name": "fasting-tracker",
    "description": "Intermittent fasting tracker",
    "slug": "fasting-tracker",
    "privacy": "unlisted",
    "sdkVersion": "33.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/res/images/logo.png",
    "splash": {
      "image": "./src/res/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.brentkeller.fastingtracker",
      "supportsTablet": true
    },
    "android": {
      "package": "com.brentkeller.fastingtracker",
      "permissions": []
    },
    "androidStatusBarColor": "#339900",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#339900"
    }
  }
}
